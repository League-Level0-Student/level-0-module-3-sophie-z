void setup(){
size(600, 600);
}
void draw(){
  for(int i=0; i<8; i++){
    ellipse(300, 300, 450, 450);
    fill(#000000);
    ellipse(300, 300, 400, 400);
    fill(#FF0000);
    ellipse(300, 300, 350, 350);
    fill(#000000);
    ellipse(300, 300, 300, 300);
    fill(#FF0000);
    ellipse(300, 300, 250, 250);
    fill(#000000);
    ellipse(300, 300, 200, 200);
    fill(#FF0000);
    ellipse(300, 300, 150, 150);
    fill(#000000);
    ellipse(300, 300, 100, 100);
    fill(#FF0000);
}
}